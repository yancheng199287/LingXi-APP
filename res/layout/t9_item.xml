<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
            android:paddingLeft="10dp"
    android:paddingRight="10dp" 
        android:layout_below="@+id/alpha" >

         <ImageView
        android:id="@+id/t9_imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="5dp"
        android:src="@drawable/ic_contact_default_1" />

    <View
        android:id="@+id/t9_view1"
        android:layout_width="wrap_content"
        android:layout_height="5dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/t9_imageView1"
        android:background="@color/black"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/t9_contactName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/t9_imageView1"
        android:layout_centerHorizontal="true"
        android:text="李琦"
     
        android:textSize="16dp" />

    <TextView
        android:id="@+id/t9_contactNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/t9_view1"
        android:layout_centerHorizontal="true"
        android:text="15927913644"
        android:textSize="16dp" />
        
        
    </RelativeLayout>

</RelativeLayout>
